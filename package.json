{
  "name": "nestjsevents",
  "version": "0.1.0",
  "private": true,
  "description": "Api events API (NestJS, Fastify, CQRS + Event Sourcing)",
  "license": "MIT",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:debug": "node --inspect-brk dist/main.js",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "migration:generate": "typeorm-ts-node-commonjs -d ./typeorm/postgres.datasource.ts migration:generate src/database/migrations/auto-migration",
    "migration:run": "typeorm-ts-node-commonjs -d ./typeorm/postgres.datasource.ts migration:run"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^13.0.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/cqrs": "^10.2.8",
    "@nestjs/microservices": "^10.4.20",
    "@nestjs/platform-fastify": "^10.4.20",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.6.1",
    "fastify": "5.4.0",
    "ioredis": "^5.7.0",
    "mongodb": "^6.18.0",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.16.3",
    "pino": "^9.8.0",
    "pino-pretty": "^11.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/testing": "^10.4.20",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.9.2"
  }
}
